<dev:ASPxGridView
	runat="server"
	CssPostfix="Soft_Orange"
	CssFilePath="~/App_Themes/Default/Dev/styles.css">
    <SettingsPager CurrentPageNumberFormat="{0}">
        <AllButton>
            <Image Height="19px" Width="28px" />
        </AllButton>
        <NextPageButton>
            <Image Height="19px" Width="19px" />
        </NextPageButton>
        <PrevPageButton>
            <Image Height="19px" Width="19px" />
        </PrevPageButton>
        <FirstPageButton>
            <Image Height="19px" Width="23px" />
        </FirstPageButton>
        <LastPageButton>
            <Image Height="19px" Width="23px" />
        </LastPageButton>
    </SettingsPager>
    <SettingsLoadingPanel Text="" />
    <Images ImageFolder="~/App_Themes/Default/Dev/Images/">
        <PopupEditFormWindowClose Height="18px" Width="18px" />
        <HeaderSortDown Height="3px" Width="6px" Url="~/App_Themes/Default/Dev/Images/gvHeaderSortDown.png" />
        <ExpandedButton Height="13px" Width="13px" Url="~/App_Themes/Default/Dev/Images/gvExpandedButton.png" />
        <CollapsedButton Height="14px" Width="13px" Url="~/App_Themes/Default/Dev/Images/gvCollapsedButton.png" />
        <HeaderFilter Height="16px" Width="16px" Url="~/App_Themes/Default/Dev/Images/gvHeaderFilter.png" />
        <DetailCollapsedButton Height="14px" Width="13px" Url="~/App_Themes/Default/Dev/Images/gvCollapsedButton.png" />
        <DetailExpandedButton Height="13px" Width="13px" Url="~/App_Themes/Default/Dev/Images/gvExpandedButton.png" />
        <HeaderActiveFilter Height="16px" Width="16px" Url="~/App_Themes/Default/Dev/Images/gvHeaderFilterActive.png" />
        <HeaderSortUp Height="3px" Width="6px" Url="~/App_Themes/Default/Dev/Images/gvHeaderSortUp.png" />
        <WindowResizer Url="~/App_Themes/Default/Dev/Images/WindowResizer.png" Width="12px" Height="12px" />
        <FilterRowButton Width="13px" Height="13px" />
    </Images>
    <ImagesEditors>
        <CalendarPrevYear Url="~/App_Themes/Default/Dev/Images/edtCalendarPrevYear.png" Width="23px" Height="19px" />
        <CalendarPrevMonth Url="~/App_Themes/Default/Dev/Images/edtCalendarPrevMonth.png" Width="19px" Height="19px" />
        <CalendarNextMonth Url="~/App_Themes/Default/Dev/Images/edtCalendarNextMonth.png" Width="19px" Height="19px" />
        <CalendarNextYear Url="~/App_Themes/Default/Dev/Images/edtCalendarNextYear.png" Width="23px" Height="19px" />
    </ImagesEditors>
    <Styles CssPostfix="Soft_Orange" CssFilePath="~/App_Themes/Default/Dev/styles.css">
        <LoadingPanel ImageSpacing="10px" />
        <Header SortingImageSpacing="5px" ImageSpacing="5px" />
    </Styles>
    <Paddings Padding="1px" />
</dev:ASPxGridView>

