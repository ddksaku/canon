<dev:ASPxRoundPanel
	runat="server"
	CssPostfix="Soft_Orange"
	CssFilePath="~/App_Themes/Default/Dev/styles.css">
    <Border BorderStyle="None" />
    <ContentPaddings PaddingTop="10px" PaddingBottom="10px" PaddingLeft="12px" />
    <HeaderStyle BackColor="#F9F9F9" Height="24px">
        <Paddings PaddingTop="1px" PaddingBottom="1px" PaddingLeft="12px" />
        <BackgroundImage Repeat="RepeatX" VerticalPosition="bottom" ImageUrl="~/App_Themes/Default/Dev/Images/rpHeaderBack.gif" />
        <Border BorderStyle="None" />
    </HeaderStyle>
    
    <%-- Corner parts --%>
    <TopLeftCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpTopLeftCorner.png" />
    <NoHeaderTopLeftCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpNoHeaderTopLeftCorner.png" />
    <TopRightCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpTopRightCorner.png" />
    <NoHeaderTopRightCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpNoHeaderTopRightCorner.png" />
    <BottomLeftCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpBottomLeftCorner.png" />
    <BottomRightCorner Height="9px" Width="9px" Url="~/App_Themes/Default/Dev/Images/rpBottomRightCorner.png" />
    
    <%-- Header parts --%>
    <HeaderLeftEdge>
        <BackgroundImage Repeat="NoRepeat" VerticalPosition="bottom" ImageUrl="~/App_Themes/Default/Dev/Images/rpHeaderLeftEdge.gif" />
    </HeaderLeftEdge>
    <HeaderContent>
        <BackgroundImage Repeat="RepeatX" VerticalPosition="bottom" ImageUrl="~/App_Themes/Default/Dev/Images/rpHeaderBack.gif" />
    </HeaderContent>
    <HeaderRightEdge>
        <BackgroundImage Repeat="NoRepeat" VerticalPosition="bottom" ImageUrl="~/App_Themes/Default/Dev/Images/rpHeaderRightEdge.gif" />
    </HeaderRightEdge>
    
    <%-- Middle parts --%>
    <Content BackColor="White" />
    <RightEdge BackColor="White">
        <BackgroundImage Repeat="RepeatY" HorizontalPosition="right" ImageUrl="~/App_Themes/Default/Dev/Images/rpRightEdge.gif" />
    </RightEdge>
    <TopEdge BackColor="White">
        <BackgroundImage ImageUrl="~/App_Themes/Default/Dev/Images/rpTopEdge.gif" />
    </TopEdge>
    <NoHeaderTopEdge BackColor="White">
        <BackgroundImage ImageUrl="~/App_Themes/Default/Dev/Images/rpNoHeaderTopEdge.gif" />
    </NoHeaderTopEdge>
    <LeftEdge BackColor="White">
        <BackgroundImage Repeat="RepeatY" HorizontalPosition="left" ImageUrl="~/App_Themes/Default/Dev/Images/rpLeftEdge.gif" />
    </LeftEdge>
    <BottomEdge BackColor="White">
        <BackgroundImage Repeat="RepeatX" VerticalPosition="bottom" ImageUrl="~/App_Themes/Default/Dev/Images/rpBottomEdge.gif" />
    </BottomEdge>
</dev:ASPxRoundPanel>

